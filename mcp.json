{
    "mcpServers": {
        "n8n-mcp": {
            "command": "node",
            "args": [
                "./n8n-mcp/dist/mcp/index.js"
            ],
            "env": {
                "MCP_MODE": "stdio",
                "LOG_LEVEL": "error",
                "DISABLE_CONSOLE_OUTPUT": "true"
            }
        }
    }
}